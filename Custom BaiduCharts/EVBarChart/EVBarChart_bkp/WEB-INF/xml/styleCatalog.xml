<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE StyleCatalog SYSTEM "styleCatalog.dtd">
<StyleCatalog>
	<Styles>
        <!-- Defining style for visualization - mojoClassName, name and description should be updated for given visualization, description is NOT the value displayed on document/dashbord-->
        <Style name="EVBarChart" description="EVBarChart - Vertical Bar Chart" transform="ReportAjaxMojoVisualizationTransform">
            <ActualParameters>
                <ActualParameter name="documentStyle" type="1" value="RWGridJsonStyle" />
                <ActualParameter name="reportXMLStyle" type="1" value="VisualizationDataStyle" />
                <ActualParameter name="mojoClassName" type="1" value="plugins.EVBarChart.EVBarChart"/>
            </ActualParameters>
            <Layouts>
                <Layout layout_source="AppLayoutSourceFile" order="1">
                    <![CDATA[/WEB-INF/xml/layouts/MojoVisualizationLayout.xml]]>
                </Layout>
            </Layouts>
        </Style>
	</Styles>
</StyleCatalog>
